+            apmib_get(MIB_MAP_CONTROLLER, (void *)&map_controller);			
+			if(1 == map_controller){				
+				system("iwpriv wlan0-vxd set_mib a4_enable=0");/*set controller vxd a4 disabled*/				
+				system("iwpriv wlan1-vxd set_mib a4_enable=0");/*set controller vxd a4 disabled*/			
+			}
+#endif
